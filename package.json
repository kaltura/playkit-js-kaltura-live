  },
  "description": "",
  "main": "dist/playkit-js-kaltura-live.js",
  "scripts": {
    "clean": "rm -rf dist",
    "reset": "npm run clean && rm -rf node_modules",
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "serve": "webpack-dev-server --open --config webpack.dev.js",
    "serve:external": "npm run serve -- --host 0.0.0.0",
    "analyze": "npm run build && npx source-map-explorer dist/playkit-js-kaltura-live.js",
    "contrib:latest": "npm i @playkit-js-contrib/{common,ui,plugin}@latest",
    "contrib:next": "npm i @playkit-js-contrib/{common,ui,plugin}@next",
    "contrib:local": "npm link @playkit-js-contrib/{common,ui,plugin} --production",
    "deploy:prepare": "node scripts/deploy-prepare.js",
    "deploy:publish-to-npm": "node scripts/deploy-publish.js",
    "deploy:next:publish-to-npm": "node scripts/deploy-publish.js",
    "deploy:next:prepare": "node scripts/deploy-prepare.js --prerelease next"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "chalk": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "inquirer": "^7.0.0",
    "lint-staged": "^10.0.2",
    "node-sass": "^4.5.3",
    "prettier": "^1.15.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "standard-version": "^7.0.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-consistent-codestyle": "^1.14.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.2.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@playkit-js-contrib/common": "^3.1.1",
    "@playkit-js-contrib/plugin": "^3.1.1",
    "@playkit-js-contrib/ui": "^3.1.1",
    "@types/node": "^8.10.59",
    "deep-diff": "^1.0.2",
    "kaltura-typescript-client": "file:libs/kaltura-typescript-client-7.0.1-v20200311-102105.tgz",
    "preact": "^8.5.3",
    "serialize-javascript": "^2.1.1",
    "svg-url-loader": "^2.3.2"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "src"
  ],
  "lint-staged": {
    "*.{ts,tsx,json,css,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
